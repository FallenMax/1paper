{
  "compilerOptions": {
    "target": "esnext",
    "module": "NodeNext",
    "sourceMap": true,
    "outDir": "../../",
    "strict": true,
    "noImplicitAny": false,
    "moduleResolution": "NodeNext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "types": ["vitest/globals"]
  },
  "include": ["./**/*"],
  "exclude": ["**/node_modules"]
}
